config  pragram "record"
	list "record_on" "amixer cset name='LADC input Mixer MIC1 boost Switch' 1"
	list "record_on" "amixer cset name='RADC input Mixer MIC1 boost Switch' 1"
	list "record_on" "amixer cset name='ADCR Mux' 'ADC'"
	list "record_on" "amixer cset name='ADCL Mux' 'ADC'"
	list "record_on" "amixer cset name='AIF1 AD0L Mixer ADCL Switch' 1"
	list "record_on" "amixer cset name='AIF1 AD0R Mixer ADCR Switch' 1"
	list "record_on" "amixer cset name='AIF1OUT0L Mux' 'AIF1_AD0L'"
	list "record_on" "amixer cset name='AIF1OUT0R Mux' 'AIF1_AD0R'"
	list "record_off" "amixer cset name='AIF1 AD0L Mixer ADCL Switch' 0"
	list "record_off" "amixer cset name='AIF1 AD0R Mixer ADCR Switch' 0"
	list "record_off" "amixer cset name='LADC input Mixer MIC1 boost Switch' 0"
	list "record_off" "amixer cset name='RADC input Mixer MIC1 boost Switch' 0"
